{
  "manifest_version": 3,
  "name": "Text Command Palette",
  "version": "1.0.0",
  "description": "VS Codeâ€“style command palette for transforming selected text anywhere.",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["activeTab"]
}
